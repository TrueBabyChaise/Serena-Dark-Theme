	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7c6d6d"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"entity.name.variable.field",
				"string constant.other.placeholder",
				"variable.other",
			],
			"settings": {
				"foreground": "#ff8cbd",
				"fontStyle": ""
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#e100ff" // IDK
			}
		},
		{
			"name": "Keyword Classic",
			"scope": [
				"keyword.type.string",
				"keyword.type.int",
				"keyword.type.float",
				"keyword.type.char",
				"keyword.type.bool",
				"keyword.type.void",
				"keyword.type.number",
				"keyword.type.object",
				"keyword.type.array",
			],
			"settings": {
				"foreground": "#77b5fe",
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#ff0055" // IDK
			}
		},
		{
			"name": "Keyword Type, Storage",
